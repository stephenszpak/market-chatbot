{
  "name": "ab-insights-widget",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:watch": "AB_WIDGET_OUTDIR=../../priv/static/ab_widget vite build --watch --mode development --emptyOutDir --clearScreen=false",
    "build": "vite build && mkdir -p ../../priv/static/ab_widget && cp -R dist/. ../../priv/static/ab_widget/",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@fortawesome/fontawesome-free": "^6.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.9.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "typescript": "^5.4.0",
    "vite": "^5.3.1"
  }
}
